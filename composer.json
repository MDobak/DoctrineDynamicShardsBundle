{
    "name": "mdobak/doctrine-dynamic-shards-bundle",
    "type": "symfony-bundle",
    "description": "Dynamic shards for Doctrine DBAL",
    "keywords": [
        "doctrine",
        "doctrine2",
        "dbal",
        "symfony",
        "symfony2",
        "symfony3",
        "symfony4",
        "bundle",
        "shard",
        "sharding"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Micha≈Ç Dobaczewski",
            "email": "mdobak@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "doctrine/doctrine-bundle": "^1.0",
        "doctrine/doctrine-cache-bundle": "^1.0",
        "doctrine/orm": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "MDobak\\DoctrineDynamicShardsBundle\\": "src"
        },
        "exclude-from-classmap": [
            "tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MDobak\\DoctrineDynamicShardsBundle\\Tests\\": "tests"
        }
    }
}
